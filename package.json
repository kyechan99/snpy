{
  "name": "snpy",
  "version": "1.0.0",
  "description": "A simple CLI tool for generating templates.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/snpy.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "tsc && node dist/dev.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyechan99/snpy.git"
  },
  "keywords": [
    "cli",
    "template",
    "generator"
  ],
  "author": "kyechan99",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyechan99/snpy/issues"
  },
  "homepage": "https://github.com/kyechan99/snpy#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix"
    ]
  }
}
